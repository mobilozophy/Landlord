{
    "name": "hipsterjazzbo/landlord",
    "description": "A simple, single database multi-tenancy solution for Laravel 5.2+",
    "license": "MIT",
    "keywords": ["tenant", "tenancy", "multitenant", "multitenancy"],
    "authors": [
        {
            "name": "Caleb Fidecaro",
            "email": "calebfidecaro@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.8.15|^6.0"
    },
    "autoload": {
        "psr-4": {
            "HipsterJazzbo\\Landlord\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HipsterJazzbo\\Landlord\\LandlordServiceProvider"
            ],
            "aliases": {
                "Landlord": "HipsterJazzbo\\Landlord\\Facades\\LandlordFacade"
            }
        }
    }
}
